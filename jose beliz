Proceso Practica_1_multiplicacion_resta	
	Definir n1,n2,n3 como Real;
	escribir "Ingresar primer nùmero";
	Leer n1;
	escribir "segundo nùmero";
	Leer n2;
	escribir "Ingresar tercer nùmero";
	Leer n3;
	S<-n1*n2*n3;
	escribir "La multiplicacion de S";
	S<-m1-n2-n3;
	escribir "la resta de S";
FinProceso
